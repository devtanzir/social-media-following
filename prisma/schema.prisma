
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model User {
  id String @id @default(auto()) @map("_id") @db.ObjectId

  name String
  surname String
  avatar String
  coverPhoto String
  
  status Boolean @default(true)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}